{
    "root": true,
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
        "jest": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "no-unused-vars" : [
            "error",
            {
                "args": "none"
            }
        ],
        "no-console": "off"
    },
    "globals": {
        "Iterable": false,
        "Generator": false
    }
}
